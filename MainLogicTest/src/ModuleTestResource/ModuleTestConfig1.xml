<?xml version="1.0" encoding="utf-8"?>
<!-- 保留关键字__OUTPORT__不能做LogicUnit的名字 -->
<Config>
    <DataTypeDefinition>
        <DataType name="32d1v" spec="[32][1]"/>
        <DataType name="32d1v1b" spec="[32][1][1]" />
    </DataTypeDefinition>

    <Module name="PE">

        <InPort specName="32d1v"/>
        <InPort specName="32d1v"/>
        <OutPort specName="32d1v"/>
        <OutPort specName="32d1v"/>

        <Component name="mem1" type="MEM">
            <Parameter specName="32d1v" size="128"/>
        </Component>

        <Logic name="inBuffer1">
            <InputSpec specName="32d1v"/>
            <OutputSpec specName="32d1v"/>
            <Parameter funName="TestFun1" cycleCounter="1"/>
        </Logic>

        <Logic name="inBuffer2">
            <InputSpec specName="32d1v"/>
            <OutputSpec specName="32d1v"/>
            <Parameter funName="TestFun1" cycleCounter="1"/>
        </Logic>

        <Logic name="inBuffer3">
            <InputSpec specName="32d1v"/>
            <OutputSpec specName="32d1v"/>
            <Parameter funName="TestFun1" cycleCounter="1"/>
        </Logic>

        <Logic name="inBuffer4">
            <InputSpec specName="32d1v"/>
            <OutputSpec specName="32d1v"/>
            <Parameter funName="TestFun1" cycleCounter="1"/>
        </Logic>

        <Connection>
            <BuildConnection sourceName="__INPORT__" sourceOutPortIndex="0" targetName="inBuffer1" targetInPortIndex="0"/>
            <BuildConnection sourceName="__INPORT__" sourceOutPortIndex="1" targetName="inBuffer4" targetInPortIndex="0"/>
            <BuildConnection sourceName="inBuffer1" sourceOutPortIndex="0" targetName="inBuffer2" targetInPortIndex="0"/>
            <BuildConnection sourceName="inBuffer2" sourceOutPortIndex="0" targetName="inBuffer3" targetInPortIndex="0"/>
            <BuildConnection sourceName="inBuffer3" sourceOutPortIndex="0" targetName="__OUTPORT__" targetInPortIndex="0"/>
            <BuildConnection sourceName="inBuffer4" sourceOutPortIndex="0" targetName="__OUTPORT__" targetInPortIndex="1"/>
        </Connection>

    </Module>
</Config>